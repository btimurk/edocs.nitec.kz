<script type="text/javascript">
    /* <![CDATA[ */

    function setCookie(name, value, options) {
        options = options || {};

        var expires = options.expires;

        if (typeof expires == "number" && expires) {
            var d = new Date();
            d.setTime(d.getTime() + expires*1000);
            expires = options.expires = d;
        }
        if (expires && expires.toUTCString) {
            options.expires = expires.toUTCString();
        }

        value = encodeURIComponent(value);

        var updatedCookie = name + "=" + value;

        for(var propName in options) {
            updatedCookie += "; " + propName;
            var propValue = options[propName];
            if (propValue !== true) {
                updatedCookie += "=" + propValue;
            }
        }

        document.cookie = updatedCookie;
    }

    function getCookie(userdata) {
        var name = userdata + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return decodeURIComponent(c.substring(name.length, c.length));
        }
        return "";
    }

    function delete_cookie(name) {
        document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }
    function setlanguage($language)
    {
        setCookie('language', $language, {path: '/', expires: 365 * 24 * 60 * 60});
        var login = document.getElementById("login").value;
        var password = document.getElementById("password").value;
        setCookie('login', login, {path: '/', expires: 365 * 24 * 60 * 60});
        setCookie('password', password, {path: '/', expires: 365 * 24 * 60 * 60});
        window.location.replace("/user/login" + window.location.search);
    }

    function getBrowser() {
        var ua=navigator.userAgent,tem,M=ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if(/trident/i.test(M[1])){
            tem=/\brv[ :]+(\d+)/g.exec(ua) || [];
            return {name:'IE',version:(tem[1]||'')};
        }
        if(M[1]==='Chrome'){
            tem=ua.match(/\bOPR|Edge\/(\d+)/)
            if(tem!=null)   {return {name:'Opera', version:tem[1]};}
        }
        M=M[2]? [M[1], M[2]]: [navigator.appName, navigator.appVersion, '-?'];
        if((tem=ua.match(/version\/(\d+)/i))!=null) {M.splice(1,1,tem[1]);}
        return {
            name: M[0] == 'IE' || M[0] == 'MSIE' ? 'Internet Explorer': M[0],
            version: M[1]
        };
    }

    function checkIsSupported(){
        var userBrowser = getBrowser();

        if(userBrowser.name == 'Chrome' && userBrowser.version < 21) return false;
        if(userBrowser.name == 'Safari' && userBrowser.version < 6) return false;
        if(userBrowser.name == 'Firefox' && userBrowser.version < 30) return false;
        if(userBrowser.name == 'Internet Explorer' && userBrowser.version < 9) return false;
        else return true;
    }
    function createNotSupported(title,text,browsers){
        $('.additional_links').hide();
        var html = '<h3 class="not-supported-title">' + title + '</h3>';
        html += '<p class="not-supported-text">'+text+'</p>';
        html += '<ul class="not-supported-body"> ';
        $.each(browsers, function(i, browser) {
            if(browser.isHidden) return;
            html += '<li>' +
                '<img src="'+browser.imageSrc+'" width="100%">' +
                '<a href="'+browser.link+'">' + browser.name + '</a>' +
                '</li>';
        });
        html += '</ul>';
        $('#page_columns').html(html);
    }
    function getFormData($form){
        var unindexed_array = $form.serializeArray();
        var indexed_array = {};
        $.map(unindexed_array, function(n, i){
            indexed_array[n['name']] = n['value'];
        });
        return indexed_array;
    }
    function isMsIe() {
        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");
