                    {
                        var sText = "\u041f\u0440\u043e\u0432\u0435\u0440\u043e\u0447\u043d\u044b\u0439 \u043a\u043e\u0434 \u043e\u0442\u043f\u0440\u0430\u0432\u043b\u0435\u043d \u043d\u0430 \u0434\u043e\u0432\u0435\u0440\u0435\u043d\u043d\u044b\u0439 \u0430\u0434\u0440\u0435\u0441";
                        sText = sText.replace('%s', data.email);
                        oTwofactorResult.addClass('old_pass').html(sText).show();
                        if(oTwofactorResult.innerHeight() > 50) {
                            oPageCol.css('margin-top', '100px');
                        }
                    });
                    $('.additional_links').hide();
                });
                break;
            case 25:
                oResult.addClass('error').html('Не заполнена почта для двухфакторной авторизации');
                break;
            case 34:
                oLoginSignForm.fadeOut(200, function()
                {
                    oLoginForm.fadeIn(200, function()
                    {
                        oLoginResult.addClass('old_pass').html('С данного аккаунта сначала нужно авторизоваться через логин/пароль').show();
                        if(oLoginResult.innerHeight() > 50) {
                            oPageCol.css('margin-top', '100px');
                        }
                    });
                    $('.additional_links').hide();
                });
                break;
            case '1':
            case 1:
            case 31:
                oResult.addClass('error').html('Пользователь не найден');
                break;
            case 32:
                oResult.addClass('error').html('Найдено более одного пользователя с данным сертификатом');
                break;
            case 22:
                oResult.addClass('error').html('Невозможно зайти с данного адреса');
                break;
            case 50:
                oResult.addClass('error').html('Согласно пункта 10 Решения Коллегии МВД Республики Казахстан, от 13 апреля 2023 года 4 дсп КМ функционирование информационных систем документационного обеспечения осуществляется с 9:00 до 18:30');
                break;
            case 26:
                oResult.addClass('error').html('Учетная запись занята другим пользователем');
                oLoginSignForm.find('[name="enter_anyway"]').val(1);
                $('#choose-cert')
                    .removeAttr('disabled')
                    .val('Все равно войти')
                    .unbind('click')
                    .click(function(){
                        oLoginSing.login();
                        return false;
                    });
                return;
            default:
                console.log(sMessageCode);
                break;
        }
        jQuery('#choose-cert').removeAttr('disabled').val('Попробовать снова');
    }

    
    /* ]]> */
</script>



